<section id="product-model-details">
  <div>
    <h1>Nome do modelo: <%= @product_model.name %></h1>
  </div>
  <div>
    <dl>
      <dt><strong>Fornecedor: </strong></dt>
        <dd><%= @product_model.supplier.brand_name %></dd>
      <dt><strong>SKU:</strong></dt>
        <dd><%= @product_model.sku %></dd>
      <dt><strong>Dimens√£o:</strong></dt>
        <dd><%= @product_model.width %>cm x <%= @product_model.height %>cm x <%= @product_model.depth %>cm</dd>
      <dt><strong>Peso:</strong></dt>
        <dd><%= @product_model.weight %>g</dd>
    </dl>
  </div>
</section>